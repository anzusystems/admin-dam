  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "production": "vite build --mode production",
    "development": "vite build --mode development --sourcemap",
    "ci": "yarn lint:eslint",
    "preview": "vite preview",
    "lint": "yarn lint:tsc ; yarn lint:eslint ; yarn lint:stylelint",
    "lint:tsc": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:eslint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:prettier": "prettier -c \"src/**/*.{ts,vue}\"",
    "lint:prettier:fix": "prettier -w \"src/**/*.{ts,vue}\"",
    "lint:stylelint": "stylelint **/*.{scss,vue}",
    "lint:stylelint:fix": "stylelint **/*.{scss,vue} --fix"
  },
  "dependencies": {
    "@anzusystems/common-admin": "^0.0.62",
    "@mdi/font": "7.2.96",
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "@vueuse/core": "10.1.0",
    "@vueuse/integrations": "10.1.0",
    "axios": "^1.3.6",
    "core-js": "^3.30.1",
    "js-sha1": "^0.6.0",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.35",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.1.15"
  },
  "devDependencies": {
    "@cypress/grep": "^3.1.5",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.16.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.10.0",
    "cypress-mochawesome-reporter": "^3.4.0",
    "eslint": "8.39.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-vue": "^9.11.0",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "stylelint": "^15.6.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "1.4.4"
  }
